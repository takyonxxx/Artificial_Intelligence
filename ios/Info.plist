<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Display name of your app -->
    <key>CFBundleDisplayName</key>
    <string>YourAppName</string>

    <!-- Executable file name -->
    <key>CFBundleExecutable</key>
    <string>${EXECUTABLE_NAME}</string>

    <!-- App icon file name (make sure it matches your asset catalog) -->
    <key>CFBundleIconFile</key>
    <string>${ASSETCATALOG_COMPILER_APPICON_NAME}</string>

    <!-- Bundle identifier (replace com.yourcompany.yourapp) -->
    <key>CFBundleIdentifier</key>
    <string>com.turkay.biliyor.Ai</string>

    <!-- Bundle name -->
    <key>CFBundleName</key>
    <string>YourAppName</string>

    <!-- Bundle package type (APPL for application) -->
    <key>CFBundlePackageType</key>
    <string>APPL</string>

    <!-- App version (replace 1.0.0) -->
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>

    <!-- Bundle signature -->
    <key>CFBundleSignature</key>
    <string>${QMAKE_PKGINFO_TYPEINFO}</string>

    <!-- Build version (replace 1) -->
    <key>CFBundleVersion</key>
    <string>1</string>

    <!-- Specify that the app requires iPhone OS -->
    <key>LSRequiresIPhoneOS</key>
    <true/>

    <!-- Minimum iOS version required (replace 11.0) -->
    <key>MinimumOSVersion</key>
    <string>11.0</string>

    <!-- Note -->
    <key>NOTE</key>
    <string>This file was generated by Qt/QMake.</string>

    <!-- Launch screen storyboard name (replace LaunchScreen if necessary) -->
    <key>UILaunchStoryboardName</key>
    <string>LaunchScreen</string>

    <!-- Supported interface orientations -->
    <key>UISupportedInterfaceOrientations</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
        <string>UIInterfaceOrientationPortraitUpsideDown</string>
        <string>UIInterfaceOrientationLandscapeLeft</string>
        <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
</dict>
</plist>

